# Database Configuration
POSTGRES_DB=tasbal
POSTGRES_USER=tasbal_user
POSTGRES_PASSWORD=tasbal_password
POSTGRES_PORT=5432

# Backend Configuration
BACKEND_PORT=8080
SPRING_PROFILES_ACTIVE=dev

# Dev Container User Configuration (set to match your host user)
# On Mac/Linux: Run `id -u` and `id -g` to get your UID and GID
UID=1000
GID=1000

# Firebase Configuration
FIREBASE_PROJECT_ID=tasbal-project
FIREBASE_CREDENTIALS_PATH=/path/to/firebase-credentials.json

# CORS Configuration
CORS_ALLOWED_ORIGINS=http://localhost:*